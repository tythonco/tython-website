{
  "name": "tython-website",
  "version": "1.0.0",
  "description": "Contains Tython website to be served as a Jekyll static site via GitHub Pages",
  "private": true,
  "dependencies": {},
  "devDependencies": {
    "coffeescript": "^2.5.1",
    "confluence-to-markdown": "git+https://github.com/tythonco/confluence-to-markdown.git",
    "open-cli": "^7.1.0"
  },
  "scripts": {
    "draft": "bundle exec jekyll draft",
    "start": "coffee node_modules/confluence-to-markdown/src/index.coffee TI out",
    "test": "bundle install && open-cli http://localhost:4000 && bundle exec jekyll serve --livereload --drafts --future"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tythonco/tython-website.git"
  },
  "author": "Scott Covert",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tythonco/tython-website/issues"
  },
  "homepage": "https://github.com/tythonco/tython-website#readme"
}
